{
  "name": "react-typescript-library-template",
  "version": "1.0.0",
  "description": "A basic template for creating react typescript library components, integrated with storybook.",
  "author": "aviklai",
  "license": "MIT",
  "repository": "react-typescript-library-template",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "module": "dist/index.es.js",
  "jsnext:main": "dist/index.es.js",
  "engines": {
    "node": ">=8",
    "npm": ">=5"
  },
  "scripts": {
    "test": "cross-env CI=1 react-scripts-ts test --env=jsdom",
    "test:watch": "react-scripts-ts test --env=jsdom",
    "build": "rollup -c",
    "start": "rollup -c -w",
    "prepare": "yarn run build",
    "predeploy": "cd example && yarn install && yarn run build",
    "deploy": "gh-pages -d example/build",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "prop-types": "^15.5.4",
    "react": "^15.0.0 || ^16.0.0",
    "react-dom": "^15.0.0 || ^16.0.0"    
  },
  "devDependencies": {
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-info": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "@svgr/rollup": "2.4.1",
    "@types/jest": "^22.2.3",
    "@types/react": "16.8.16",
    "@types/react-dom": "16.8.4",
    "@types/react-test-renderer": "^16.8.1",
    "@types/storybook__react": "^4.0.1",
    "awesome-typescript-loader": "^5.2.1",
    "babel-core": "6.26.3",
    "babel-loader": "^8.0.6",
    "babel-runtime": "6.26.0",
    "cross-env": "5.1.4",
    "gh-pages": "1.2.0",
    "jest": "^24.8.0",
    "node-sass": "^4.12.0",
    "react": "16.8.6",
    "react-docgen-typescript-loader": "^3.1.0",
    "react-dom": "16.8.6",
    "react-scripts-ts": "2.16.0",
    "react-test-renderer": "^16.8.6",
    "rollup": "1.12.5",
    "rollup-plugin-babel": "3.0.7",
    "rollup-plugin-commonjs": "9.1.3",
    "rollup-plugin-node-resolve": "3.4.0",
    "rollup-plugin-peer-deps-external": "2.2.0",
    "rollup-plugin-postcss": "2.0.3",
    "rollup-plugin-scss": "^1.0.1",
    "rollup-plugin-typescript2": "0.21.1",
    "rollup-plugin-url": "2.2.1",
    "sass-loader": "^7.1.0",
    "ts-jest": "^22.4.2",
    "ts-loader": "^6.0.2",
    "tslib": "1.9.3",
    "typescript": "3.4.1"
  },
  "files": [
    "dist"
  ]  
}
